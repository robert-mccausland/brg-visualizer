{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "airbnb/hooks"
  ],
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "max-len": [
      "error",
      {
        "code": 140
      }
    ]
  },
  "globals": {
    "window": "readonly",
    "document": "readonly",
    "it": "readonly",
    "describe": "readonly"
  }
}
